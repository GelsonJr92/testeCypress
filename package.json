{
  "name": "meustestes",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "npm run cy:run",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox",
    "cy:run:edge": "cypress run --browser edge",
    "cy:run:headless": "cypress run --headless",
    "cy:run:api": "cypress run --spec \"cypress/e2e/backend/**/*\"",
    "cy:run:frontend": "cypress run --spec \"cypress/e2e/frontend/**/*\"",
    "cy:run:component": "cypress run --component",
    "cy:run:smoke": "cypress run --env grepTags=@smoke",
    "cy:run:regression": "cypress run --env grepTags=@regression",
    "cy:run:parallel": "cypress run --record --parallel --group \"Parallel Tests\"",
    "cy:open:api": "cypress open --e2e --spec \"cypress/e2e/backend/**/*\"",
    "cy:open:frontend": "cypress open --e2e --spec \"cypress/e2e/frontend/**/*\"",
    "cy:open:component": "cypress open --component",
    "report:merge": "mochawesome-merge cypress/reports/*.json > cypress/reports/merged.json",
    "report:generate": "marge cypress/reports/merged.json --reportDir cypress/reports --inline",
    "report:open": "start cypress/reports/merged.html",
    "clean:reports": "rimraf cypress/reports cypress/screenshots cypress/videos",
    "clean:cache": "cypress cache clear",
    "install:cypress": "cypress install",
    "verify:cypress": "cypress verify",
    "info:cypress": "cypress info",
    "start:test": "echo \"Start your application server here\"",
    "start:api": "echo \"Start your API server here\"",
    "lint": "echo \"Add your linting command here\"",
    "format": "echo \"Add your formatting command here\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@cypress/grep": "^4.1.0",
    "@faker-js/faker": "^9.8.0",
    "@types/node": "^22.15.22",
    "cypress": "^14.4.0",
    "cypress-mochawesome-reporter": "^3.8.2",
    "cypress-multi-reporters": "^2.0.5",
    "cypress-real-events": "^1.14.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^5.0.0",
    "mochawesome-report-generator": "^6.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
